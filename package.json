{
  "name": "react-webapp-boilerplate",
  "displayName": "React WebApp",
  "version": "0.0.0",
  "description": "React Boilerplate for Modern WebApp",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --env.prod --progress",
    "commitmsg": "commitlint -e",
    "coverage": "jest --coverage",
    "cz": "git-cz",
    "dev": "webpack-dev-server --debug",
    "format": "prettier --write",
    "lint": "run-p lint:**",
    "lint:css": "stylelint --fix src",
    "lint:js": "eslint --fix .",
    "start": "pushstate-server",
    "postmerge": "npm install",
    "precommit": "lint-staged",
    "prepush": "run-s lint test",
    "test": "cross-env NODE_ENV=test run-p test:**",
    "test:unit": "jest --notify"
  },
  "keywords": ["Boilerplate", "React", "React-Router", "Redux", "PWA", "Jest"],
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^2.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@justeat/stylelint-config-fozzie": "^2.0.1",
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-pipeline": "^0.1.0",
    "babel-polyfill": "^7.0.0-beta.3",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-preset-stage-0": "^7.0.0-beta.3",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-config-xo-space": "^0.18.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^6.3.0",
    "file-loader": "^1.1.11",
    "happypack": "^4.0.1",
    "html-critical-webpack-plugin": "^1.1.0",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "img-loader": "^2.0.1",
    "jest": "^22.4.3",
    "jest-context": "^2.1.0",
    "json5-loader": "^1.0.1",
    "lint-staged": "^7.0.0",
    "name-all-modules-plugin": "^1.0.1",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "offline-plugin": "^4.9.0",
    "postcss": "^6.0.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-logical-props": "^0.1.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-mq-keyframes": "^0.3.0",
    "postcss-reporter": "^5.0.0",
    "postcss-short": "^4.1.0",
    "postcss-url": "^7.3.1",
    "prettier": "^1.11.1",
    "pushstate-server": "^3.0.1",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.0.0",
    "resolve-url-loader": "^2.3.0",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-idiomatic-sass": "^1.0.0",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-scss": "^2.5.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "svg-sprite-loader": "^3.7.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webapp-webpack-plugin": "^1.3.1",
    "webpack": "^3.11.0",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "worker-loader": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "webpack": {
      "favicon": "./src/favicon.svg",
      "manifestFile": "./public/react-webapp.json",
      "outputPath": "./public",
      "publicPath": "/"
    }
  }
}
